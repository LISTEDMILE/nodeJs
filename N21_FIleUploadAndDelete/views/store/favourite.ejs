<%- include('../partials/head')%>
<link rel="stylesheet" href="/style.css"/>
    <body>
      <%- include('../partials/nav')%>
        <div id="body">
    <div id="div">
        <h1 id="top">Here are the added homes..</h1>
        <div >
        <% favouriteHomes.forEach(function(detail) { %>
            <li>
                <h1 id="name">
                    <%= detail.houseName %>
                    
                </h1>
                <h1 id="name">
                    <%= detail.housePrice %>
                    
                </h1>
                <h1 id="name">
                    <%= detail.houseLocation %>
                    
                </h1>
                <p id="textarea">
                    <%= detail.description %>
                </p>
                <% detail.fav = true; %>
                <%- include('../partials/favouriteButton',{home:detail})%>
            </li>
        <% });%>
    </div>
    <img src={detail.photo} alt="House Image" id="image"/>
        <p> from / <b><u> test </u></b>/addHome</p>
    </div>
</div>
<%- include ('../partials/footer')%>
    </body>
</html>